    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "antd": "^5.26.2",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "connect-pg-simple": "^10.0.0",
    "crypto": "^1.0.1",
    "drizzle-kit": "^0.31.2",
    "drizzle-orm": "^0.44.2",
